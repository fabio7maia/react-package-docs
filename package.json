{
  "private": true,
  "name": "react-packages-docs",
  "description": "React published public packages documentation",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch && postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "contentlayer": "^0.0.33",
    "front-matter": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight": "^0.14.0",
    "remix": "^1.0.6",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-highlight": "^0.12.5",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "daisyui": "^1.19.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-code-titles": "^1.0.3",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.0.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
